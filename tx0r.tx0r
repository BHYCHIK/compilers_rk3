        OPERATE AC,CLEAR,AC// sum = 0
        STORE sum
loop:          OPERATE READ,COPY,AC // dataum = READ
        STORE datum
        OPERATE EOF,COPY,AC
        TRANSFER end_loop
        OPERATE AC,CLEAR,AC
        ADD datum
        ADD sum
        STORE sum
        OPERATE AC,CLEAR,AC
        ADD minus_one
        TRANSFER loop
end_loop:   OPERATE AC,CLEAR,AC
        ADD sum
        OPERATE AC,COPY,HALT
sum:    WORD    0
datum:  WORD    0
minus_one: WORD 0
